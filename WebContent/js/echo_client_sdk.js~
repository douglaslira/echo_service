
  function echo() {
    var transport = new Thrift.Transport("http://localhost:8080/EchoService/EchoServlet");
    var protocol  = new Thrift.Protocol(transport);
    var client    = new EchoServiceClient(protocol);

    try {
      result = client.echo("Hello World!");
      alert(result);
    } catch(ouch){
      $('#result').val("Error");
    }
  }
